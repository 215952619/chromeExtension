{
    "manifest_version": 2,
    "name": "__MSG_extName__",
    "homepage_url": "https://github.com/215952619/chromeExtension/tree/master",
    "description": "A Vue Browser Extension",
    "default_locale": "en",
    "permissions": [
        "contextMenus",
        "tabs",
        "activeTab",
        "notifications",
        "webRequestBlocking",
        "storage",
        "<all_urls>",
        "*://*/*"
    ],
    "web_accessible_resources": ["js/inject-script.js"],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/content-script.js"],
            "run_at": "document_start"
        }
    ],
    "devtools_page": "devtools.html",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "__MSG_extName__",
        "default_icon": {
            "19": "icons/19.png",
            "38": "icons/38.png"
        }
    },
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    },
    "chrome_url_overrides": {
        "newtab": "override.html"
    }
}
